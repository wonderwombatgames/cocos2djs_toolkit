##
#  <p>ww.LoaderScene is a scene that you can load it when you loading files</p>
#  <p>ww.LoaderScene can present thedownload progress </p>
#  @class
#  @extends cc.Scene
#

## replaces cocos 2d-x logo on loading scene
cc._loaderImage = "data:image/jpeg;base64,"

## wonder wombat namespace
ww = ww || {}

ww.LoaderScene = cc.Scene.extend(
  _interval : null
  _label : null
  _className:"LoaderScene"

##-------------------------------------------------------------------
  ##
  # Contructor of cc.LoaderScene
  # @returns {boolean}
  #
  init: () ->
    self = this

    ## bg
    bgLayer = self._bgLayer = new cc.LayerColor(cc.color(16, 64, 32, 255))
    self.addChild(bgLayer, 0)

    ##logo
    logoWidth = 512
    logoHeight = 512

    ##image move to CCSceneFile.js
    fontSize = 80
    lblHeight =  -logoHeight / 2 + 100

    if(cc._loaderImage)
      ##loading logo
      cc.loader.loadImg(cc._loaderImage, {isCrossOrigin : true }, (err, img) -> 
          logoWidth = img.width
          logoHeight = img.height
          self._initStage(img, cc.visibleRect.center)
      )
      lblHeight = -logoHeight / 2 - 150
    
    ##loading percent
    label = self._label = new cc.LabelTTF("Loading... 0%", "Arial", fontSize)
    label.setPosition(cc.pAdd(cc.visibleRect.center, cc.p(0, lblHeight)))
    label.setColor(cc.color(200, 255, 255))
    bgLayer.addChild(@_label, 10)

    return true ## end init

##-------------------------------------------------------------------
  _initStage: (img, centerPos) ->
    self = this
    texture2d = self._texture2d = new cc.Texture2D()
    texture2d.initWithElement(img)
    texture2d.handleLoadedTexture()
    logo = self._logo = new cc.Sprite(texture2d)
    logo.setScale(cc.contentScaleFactor())
    #logo.setScale(8)
    logo.x = centerPos.x
    logo.y = centerPos.y
    self._bgLayer.addChild(logo, 10)

    return ## end _initStage


##-------------------------------------------------------------------
  # 
  # custom onEnter
  # 
  onEnter: () ->
    self = this
    cc.Node.prototype.onEnter.call(self)
    self.schedule(self._startLoading, 0.3)

    return ## end onEnter


##-------------------------------------------------------------------
  #
  # custom onExit
  #
  onExit: () ->
    cc.Node.prototype.onExit.call(this)
    tmpStr = "Loading... 0%"
    @_label.setString(tmpStr)
  
    return ## end onExit

##-------------------------------------------------------------------
  #
  # init with resources
  # @param {Array} resources
  # @param {Function|String} cb
  #
  initWithResources: (resources, cb) ->
    if(cc.isString(resources))
        resources = [resources]
    @resources = resources || []
    @cb = cb
  
    return ## end initWithResources

##-------------------------------------------------------------------
  _startLoading: () ->
    self = this
    self.unschedule(self._startLoading)
    res = self.resources
    cc.loader.load(res,
      (result, count, loadedCount) ->
        percent = (loadedCount / count * 100) | 0
        percent = Math.min(percent, 100)
        self._label.setString("Loading... " + percent + "%")
        return
      () ->
        if (self.cb)
            self.cb()
        return
      )

    return ## end _startLoading
  
) ## end class

#
# <p>cc.LoaderScene.preload can present a loaderScene with download progress.</p>
# <p>when all the resource are downloaded it will invoke call function</p>
# @param resources
# @param cb
# @returns {ww.LoaderScene|*}

cc.LoaderScene.preload = (resources, cb) ->
  _cc = cc
  if(!_cc.loaderScene)
    _cc.loaderScene = new ww.LoaderScene()
    _cc.loaderScene.init()

  _cc.loaderScene.initWithResources(resources, cb)
  cc.director.runScene(_cc.loaderScene)

  return _cc.loaderScene
